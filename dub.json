    "name": "kassios",
    "configurations": [
        {
            "name": "riscv32",
            "versions": [
                "Qemu",
                "Riscv32",
                "Riscv32Debug",
                "FeatureFloatPoint"
            ],
            "dflags-ldc": [
                "-mtriple=riscv32-unknown-elf",
                "--unittest",
                "--mabi=ilp32f"
            ]
        },
        {
            "name": "riscv32-release",
            "versions": [
                "Qemu",
                "Riscv32",
                "FeatureFloatPoint"
            ],
            "dflags-ldc": [
                "-mtriple=riscv32-unknown-elf",
                "-Oz",
                "--mabi=ilp32f"
            ]
        },
        {
            "name": "riscv64",
            "versions": [
                "Qemu",
                "Riscv64",
                "Riscv64Debug",
                "FeatureFloatPoint",
                "PTS64"
            ],
            "dflags-ldc": [
                "-mtriple=riscv64-unknown-elf",
                "--unittest",
                "-mattr=+m,+a,+c,+f,+d",
                "--mabi=lp64d"
            ]
        },
        {
            "name": "riscv64-release",
            "versions": [
                "Qemu",
                "Riscv64",
                "FeatureFloatPoint",
                "PTS64"
            ],
            "dflags-ldc": [
                "-mtriple=riscv64-unknown-elf",
                "-Oz",
                "-mattr=+m,+a,+c,+f,+d",
                "--mabi=lp64d"
            ]
        }
    ],
    "dflags-ldc": [
        "-betterC",
        "-mattr=+m,+a,+c,+f",
        "--code-model=medium",
        "--nodefaultlib",
        "--function-sections",
        "--data-sections",
        "--relocation-model=static",
        "-dip1000", 
        "-preview=in"
    ],
    "targetType": "library",
    "targetName": "kernel",
    "targetPath": "./build",
    "sourcePaths": [
        "./src"
    ],
    "importPaths": [
        "./src"
    ],
    "dependencies": {}
}